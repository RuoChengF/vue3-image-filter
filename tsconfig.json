{
    "compilerOptions": {
      "moduleResolution": "node",
      "baseUrl": ".",
      "esModuleInterop": true,
      "noEmit": true,
      "experimentalDecorators": true,
      "downlevelIteration": true,
    //   "types": ["vue-router"],
      // "allowImportingTsExtensions": true,
      "paths": {
        "@/*": ["./src/*"],
        "@/assets": ["./assets/*"],
        "@/hooks/element/*": ["src/hooks/data/*"],
        "@/store/helpers/*": ["./src/store/helpers/*"],
        "@/components/*": ["./src/components/*"],
        "@/hooks/*": ["./src/hooks/*"],
        "vue-router": ["./node_modules/vue-router"]
      }
    },
    "include": ["*", "src/**/*.ts", "src/**/*.js", "src/**/*.vue", "src/**/*.tsx", "src/**/*.d.ts", "src/models/*.ts"],
    "exclude": ["node_modules"]
  }
